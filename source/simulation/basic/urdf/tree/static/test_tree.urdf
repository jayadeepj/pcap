<?xml version="1.0"?>
<robot name="robot_with_friction">


    <!-- This first link called "world" is empty -->
    <link name="world"></link>


    <!-- A simple fixed joint from our empty world link, to our base. -->
    <!-- The base origin is offset from the world origin. -->
    <joint name="base_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="world"/>
        <child link="base_link"/>
    </joint>


    <link name="base_link">
        <visual>
            <!-- origin z axis half of cylinder length -->
            <origin xyz="0 0 1.0"/>
            <geometry>
                <cylinder length="2.0" radius="0.2"/>
            </geometry>
            <material name="orange">
                <color rgba="0.1 0.2 0.2 1"/>
            </material>
        </visual>

        <collision>
            <!-- collision origin & geometry = visual-->
            <origin xyz="0 0 1.0"/>
            <geometry>
                <cylinder length="2.0" radius="0.2"/>
            </geometry>
        </collision>


    </link>

    <!-- child joint & branch 11  Level 1, Child 1 (right)-->
    <joint name="base_to_branch11" type="revolute">
        <parent link="base_link"/>
        <child link="branch11"/>
        <!-- y=parent branch(N-1) radius
        or best to leave at 0, z = height of parent - 0.1 -->
        <origin xyz="0 0 1.9" rpy="-1.05 0 0"/>
        <axis xyz="1 0 0"/>
        <!-- The limits are w.r.t the origin angle. origin rpy is the baseline (0)-->
        <!-- <limit lower="-0.79" upper="0.79"/> -->
        <limit lower="-0.35" upper="0.35"/>
        <dynamics damping="100" friction="100"/>
    </joint>


    <link name="branch11">
        <visual>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
            <material name="blah">
                <color rgba="0.1 0.2 0.2 1"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="35.726"/>
            <inertia ixx="6.266" ixy="0" ixz="0" iyy="6.266" iyz="0" izz="0.355"/>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </inertial>
    </link>

    <!-- child joint & branch 12 Level 1, Child 2 (left)  -->
    <!-- TBD Start -->

    <joint name="base_to_branch12H1" type="revolute">
        <parent link="base_link"/>
        <child link="branch12H1"/>
        <!-- y=parent branch(N-1) radius or best to leave at 0 , z = height of parent - 0.1
         60 degree angle change sign for left, no change for axis and limits
         -->
        <origin xyz="0 0 1.9" rpy="1.05 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.35" upper="0.35"/>
        <dynamics damping="100" friction="100"/>
    </joint>


    <link name="branch12H1">
        <visual>
            <geometry>
                <cylinder length="0.2" radius="0.141"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
            <material name="blah">
                <color rgba="0.1 0.2 0.2 1"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="0.2" radius="0.141"/>
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="2.498"/>
            <inertia ixx="0.021" ixy="0" ixz="0" iyy="0.021" iyz="0" izz="0.025" />
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
        </inertial>
    </link>



    <joint name="branch12H1_to_branch12" type="revolute">
        <parent link="branch12H1"/>
        <child link="branch12"/>
        <!-- y=parent branch(N-1) radius or best to leave at 0 , z = height of parent - 0.1
         60 degree angle change sign for left, no change for axis and limits
         -->
        <origin xyz="0 0 0.1" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-0.65" upper="0.65"/>
        <dynamics damping="100" friction="100"/>
    </joint>


    <link name="branch12">
        <visual>
            <geometry>
                <cylinder length="1.42" radius="0.141"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.71" rpy="0 0 0"/>
            <material name="blah1">
                <color rgba="0.1 0.2 0.2 0.8"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="1.42" radius="0.141"/>
            </geometry>
            <origin xyz="0 0 0.71" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="17.738"/>
            <inertia ixx="3.069" ixy="0" ixz="0" iyy="3.069" iyz="0" izz="0.176" />
            <origin xyz="0 0 0.71" rpy="0 0 0"/>
        </inertial>
    </link>


    <!-- TBD End -->
    <!-- child joint & branch 21 Level 2, Child 1 (right)-->
    <joint name="branch11_to_branch21" type="revolute">
        <parent link="branch11"/>
        <child link="branch21"/>
        <!-- y=parent branch(N-1) radius or best to leave at 0, z = height of parent - 0.1 -->
        <origin xyz="0 0 1.33" rpy="-1.05 0 0"/>
        <axis xyz="1 0 0"/>
        <!-- The limits are w.r.t the origin angle. origin rpy is the baseline (0)-->
        <!-- <limit lower="-0.79" upper="0.79"/> -->
        <limit lower="-0.35" upper="0.35"/>
        <dynamics damping="26.792" friction="26.792"/>
    </joint>


    <link name="branch21">
        <visual>
            <geometry>
                <cylinder length="1.066" radius="0.1"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.533" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 0.2 0.2 0.8"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="1.066" radius="0.1"/>
            </geometry>
            <origin xyz="0 0 0.533" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="13.396"/>
            <inertia ixx="1.302" ixy="0" ixz="0" iyy="1.302" iyz="0" izz="0.067"/>
            <origin xyz="0 0 0.533" rpy="0 0 0"/>
        </inertial>
    </link>




    <!-- child joint & branch 22 Level 2, Child 2 (left)-->
    <joint name="branch11_to_branch22" type="revolute">
        <parent link="branch11"/>
        <child link="branch22"/>
        <!-- y=parent branch(N-1) radius or best to leave at 0, z = height of parent - 0.1 -->
        <origin xyz="0 0 1.33" rpy="1.05 0 0"/>
        <axis xyz="1 0 0"/>
        <!-- The limits are w.r.t the origin angle. origin rpy is the baseline (0)-->
        <!-- <limit lower="-0.79" upper="0.79"/> -->
        <limit lower="-0.35" upper="0.35"/>
        <dynamics damping="26.792" friction="26.792"/>
    </joint>


    <link name="branch22">
        <visual>
            <geometry>
                <cylinder length="1.066" radius="0.1"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.533" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 0.2 0.2 0.8"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="1.066" radius="0.1"/>
            </geometry>
            <origin xyz="0 0 0.533" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="13.396"/>
            <inertia ixx="1.302" ixy="0" ixz="0" iyy="1.302" iyz="0" izz="0.067"/>
            <origin xyz="0 0 0.533" rpy="0 0 0"/>
        </inertial>
    </link>


</robot>


