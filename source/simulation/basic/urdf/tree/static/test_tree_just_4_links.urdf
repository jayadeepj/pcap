<?xml version="1.0"?>
<robot name="robot_with_friction">


    <!-- This first link called "world" is empty -->
    <link name="world"></link>


    <!-- A simple fixed joint from our empty world link, to our base. -->
    <!-- The base origin is offset from the world origin. -->
    <joint name="base_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="world"/>
        <child link="base_link"/>
    </joint>


    <link name="base_link">
        <visual>
            <!-- origin z axis half of cylinder length -->
            <origin xyz="0 0 1.0"/>
            <geometry>
                <cylinder length="2.0" radius="0.2"/>
            </geometry>
            <material name="orange">
                <color rgba="0.1 0.2 0.2 1"/>
            </material>
        </visual>

        <collision>
            <!-- collision origin & geometry = visual-->
            <origin xyz="0 0 1.0"/>
            <geometry>
                <cylinder length="2.0" radius="0.2"/>
            </geometry>
        </collision>


    </link>


    <joint name="base_to_branch11" type="revolute">
        <parent link="base_link"/>
        <child link="branch11"/>
        <!-- y=parent branch(N-1) radiusor best to leave at 0, z = height of parent - 0.1 -->
        <origin xyz="0 0 1.9" rpy="-1.05 0 0"/>
        <axis xyz="1 0 0"/>
        <!-- The limits are w.r.t the origin angle. origin rpy is the baseline (0)-->
        <!-- <limit lower="-0.79" upper="0.79"/> -->
        <limit lower="-0.35" upper="0.35"/>
        <dynamics damping="71.452" friction="71.452"/>
    </joint>


    <link name="branch11">
        <visual>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 0.2 0.2 0.8"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="35.726"/>
            <inertia ixx="6.266" ixy="0" ixz="0" iyy="6.266" iyz="0" izz="0.355"/>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </inertial>
    </link>

    <!-- child branch 2-->


    <joint name="base_to_branch12" type="revolute">
        <parent link="base_link"/>
        <child link="branch12"/>
        <!-- y=parent branch(N-1) radius or best to leave at 0 , z = height of parent - 0.1
         60 degree angle change sign for left, no change for axis and limits
         -->
        <origin xyz="0 0 1.9" rpy="1.05 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-0.35" upper="0.35"/>
        <dynamics damping="71.452" friction="71.452"/>
    </joint>


    <link name="branch12">
        <visual>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 0.2 0.2 0.8"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="35.726"/>
            <inertia ixx="6.266" ixy="0" ixz="0" iyy="6.266" iyz="0" izz="0.355"/>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </inertial>
    </link>


    <!--Child Branch 3 -->


    <joint name="base_to_branch13" type="revolute">
        <parent link="base_link"/>
        <child link="branch13"/>
        <!-- y=parent branch(N-1) radiusor best to leave at 0, z = height of parent - 0.1 -->
        <origin xyz="0 0 1.9" rpy="0 -1.05 0"/>
        <axis xyz="0 1 0"/>
        <!-- The limits are w.r.t the origin angle. origin rpy is the baseline (0)-->
        <!-- <limit lower="-0.79" upper="0.79"/> -->
        <limit lower="-0.35" upper="0.35"/>
        <dynamics damping="71.452" friction="71.452"/>
    </joint>


    <link name="branch13">
        <visual>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>

            <!-- the visual origin is w.r.t to link origin (written in joint )
            z = length of this link/2
            -->
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 0.2 0.2 0.8"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <cylinder length="1.43" radius="0.141"/>
            </geometry>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </collision>

        <inertial>

            <mass value="35.726"/>
            <inertia ixx="6.266" ixy="0" ixz="0" iyy="6.266" iyz="0" izz="0.355"/>
            <origin xyz="0 0 0.715" rpy="0 0 0"/>
        </inertial>
    </link>




</robot>


        <!--

        pi/4 : 0.79
        pi/2 : 1.57
        pi : 3.14
        3pi/4 : 2.36

